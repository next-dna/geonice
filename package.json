{
  "name": "geo-lite",
  "version": "0.1.4",
  "description": "TypeScript geocoding + IP geolocation library for Node/Next.js (Nominatim + ipapi)",
  "keywords": [
    "geocoding",
    "geolocation",
    "ip",
    "typescript",
    "node",
    "nextjs"
  ],
  "license": "MIT",
  "author": {
    "name": "Sandip Gami",
    "url": "https://github.com/next-dna"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/next-dna/geo-lite.git"
  },
  "bugs": {
    "url": "https://github.com/next-dna/geo-lite/issues"
  },
  "homepage": "https://github.com/next-dna/geo-lite#readme",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "geo-lite": "dist/cli.cjs"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc && tsx ./scripts/build-cjs.ts",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run lint && npm run test && npm run build",
    "check": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint .",
    "format": "prettier -w .",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/node": "^20.19.22",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.10",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
