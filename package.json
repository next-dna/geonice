{
  "name": "quick-geocode",
  "version": "0.2.0",
  "description": "TypeScript geocoding + IP geolocation library for Node/Next.js (Nominatim + ipapi)",
  "keywords": [
    "geocode",
    "geocoding",
    "geolocation",
    "maps",
    "openstreetmap",
    "latitude",
    "longitude",
    "address",
    "city",
    "state",
    "country",
    "zip",
    "postal",
    "zipcode",
    "postalcode",
    "postal code",
    "ip",
    "typescript",
    "node",
    "nextjs"
  ],
  "license": "MIT",
  "author": {
    "name": "Sandip Gami",
    "url": "https://github.com/next-dna"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/next-dna/quick-geocode.git"
  },
  "bugs": {
    "url": "https://github.com/next-dna/quick-geocode/issues"
  },
  "homepage": "https://github.com/next-dna/quick-geocode#readme",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "quick-geocode": "dist/cli.cjs"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc && tsx ./scripts/build-cjs.ts",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run lint && npm run test && npm run build",
    "check": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint .",
    "format": "prettier -w .",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "server": "tsx src/server.ts",
    "server:prod": "tsx src/server-production.ts",
    "server:simple": "tsx src/server-simple.ts",
    "start": "node dist/server.cjs",
    "start:prod": "node dist/server-production.cjs",
    "start:simple": "node dist/server-simple.cjs",
    "load-test": "node scripts/load-test.js",
    "load-test:heavy": "CONCURRENT_USERS=500 REQUESTS_PER_USER=20 node scripts/load-test.js"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "prom-client": "^15.1.0",
    "redis": "^4.6.12"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.22",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.10",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
